{
  "name": "tarosse-vuenext",
  "version": "1.0.0",
  "private": true,
  "description": "taro demo",
  "templateInfo": {
    "name": "vue",
    "typescript": true,
    "css": "sass"
  },
  "scripts": {
    "build:weapp": "taro build --type weapp",
    "build:swan": "taro build --type swan",
    "build:alipay": "taro build --type alipay",
    "build:tt": "taro build --type tt",
    "build:h5": "taro build --type h5",
    "build:rn": "taro build --type rn",
    "build:qq": "taro build --type qq",
    "build:quickapp": "taro build --type quickapp",
    "dev:weapp": "npm run build:weapp -- --watch",
    "dev:swan": "npm run build:swan -- --watch",
    "dev:alipay": "npm run build:alipay -- --watch",
    "dev:tt": "npm run build:tt -- --watch",
    "dev:h5": "npm run build:h5 -- --watch",
    "dev:rn": "npm run build:rn -- --watch",
    "dev:qq": "npm run build:qq -- --watch",
    "dev:quickapp": "npm run build:quickapp -- --watch",
    "prepare": "husky install",
    "preinstall": "npx only-allow yarn",
    "commit": "npx git-cz",
    "lint:lint-staged": "lint-staged",
    "lint:eslint": "eslint --fix --cache \"**/*.{js,jsx,ts,tsx,vue}\"",
    "lint:stylelint": "stylelint --fix --cache \"**/*.{css,scss,less,styl,vue}\"",
    "lint:prettier": "prettier --write  \"**/*.{css,scss,less,styl,js,jsx,ts,tsx,vue}\""
  },
  "browserslist": [
    "last 3 versions",
    "Android >= 4.1",
    "ios >= 8"
  ],
  "author": "https://github.com/haiweilian",
  "license": "MIT",
  "dependencies": {
    "@babel/runtime": "^7.7.7",
    "@tarojs/components": "3.4.1",
    "@tarojs/plugin-framework-vue3": "3.4.1",
    "@tarojs/plugin-html": "^3.4.2",
    "@tarojs/plugin-mock": "^0.0.9",
    "@tarojs/runtime": "3.4.1",
    "@tarojs/taro": "3.4.1",
    "pinia": "^2.0.6",
    "taro-plugin-pinia": "^1.0.0",
    "vue": "^3.2.24"
  },
  "devDependencies": {
    "@babel/core": "^7.8.0",
    "@bfehub/eslint-config-vue": "^1.0.4",
    "@bfehub/stylelint-config-basic": "^1.0.4",
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@tarojs/mini-runner": "3.4.1",
    "@tarojs/webpack-runner": "3.4.1",
    "@types/jest": "^27.4.1",
    "@types/webpack-env": "^1.13.6",
    "@vue/test-utils": "^2.0.0-rc.17",
    "@vue/vue3-jest": "^27.0.0-alpha.4",
    "babel-preset-taro": "3.4.1",
    "commitizen": "^4.2.4",
    "cz-conventional-changelog": "^3.3.0",
    "eslint": "^8.10.0",
    "husky": "^7.0.4",
    "jest": "^27.5.1",
    "jest-transform-stub": "^2.0.0",
    "lint-staged": "^12.3.4",
    "postcss": "^8.4.7",
    "prettier": "^2.5.1",
    "stylelint": "^14.5.3",
    "ts-jest": "^27.1.3",
    "typescript": "^4.6.2",
    "vue-loader": "^16.8.3"
  }
}
